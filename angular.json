{
  "version": 1,
  "projects": {
    "demo": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/demo",
      "sourceRoot": "apps/demo/src",
      "prefix": "kikstart-ui",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "stylePreprocessorOptions": {
              "includePaths": ["libs"]
            },
            "outputPath": "dist/apps/demo",
            "index": "apps/demo/src/index.html",
            "main": "apps/demo/src/main.ts",
            "polyfills": "apps/demo/src/polyfills.ts",
            "tsConfig": "apps/demo/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/demo/src/favicon.ico", "apps/demo/src/assets"],
            "styles": ["apps/demo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/demo/src/environments/environment.ts",
                  "with": "apps/demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "demo:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/demo/tsconfig.app.json",
              "apps/demo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/demo/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/demo/jest.config.js",
            "tsConfig": "apps/demo/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/demo/src/test-setup.ts"
          }
        }
      }
    },
    "demo-e2e": {
      "root": "apps/demo-e2e",
      "sourceRoot": "apps/demo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/demo-e2e/cypress.json",
            "tsConfig": "apps/demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/demo-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/demo-e2e/**"]
          }
        }
      }
    },
    "schematics": {
      "root": "libs/schematics",
      "sourceRoot": "libs/schematics/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/schematics/tsconfig.lib.json",
              "libs/schematics/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/schematics/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/schematics/jest.config.js",
            "tsConfig": "libs/schematics/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/schematics",
            "tsConfig": "libs/schematics/tsconfig.lib.json",
            "packageJson": "libs/schematics/package.json",
            "main": "libs/schematics/src/index.ts",
            "assets": [
              "libs/schematics/*.md",
              {
                "input": "./libs/schematics/src",
                "glob": "**/*.!(ts)",
                "output": "./src"
              },
              {
                "input": "./libs/schematics",
                "glob": "collection.json",
                "output": "."
              },
              {
                "input": "./libs/schematics",
                "glob": "builders.json",
                "output": "."
              }
            ]
          }
        }
      }
    },
    "schematics-e2e": {
      "projectType": "application",
      "root": "apps/schematics-e2e",
      "sourceRoot": "apps/schematics-e2e/src",
      "architect": {
        "e2e": {
          "builder": "@nrwl/nx-plugin:e2e",
          "options": {
            "target": "schematics:build",
            "npmPackageName": "@kikstart-ui/schematics",
            "pluginOutputPath": "dist/libs/schematics",
            "jestConfig": "apps/schematics-e2e/jest.config.js",
            "tsSpecConfig": "apps/schematics-e2e/tsconfig.spec.json"
          }
        }
      }
    },
    "create-kikstart-ui": {
      "root": "libs/create-kikstart-ui",
      "sourceRoot": "libs/create-kikstart-ui/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/create-kikstart-ui/tsconfig.lib.json",
              "libs/create-kikstart-ui/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/create-kikstart-ui/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/create-kikstart-ui/jest.config.js",
            "tsConfig": "libs/create-kikstart-ui/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/create-kikstart-ui",
            "tsConfig": "libs/create-kikstart-ui/tsconfig.lib.json",
            "packageJson": "libs/create-kikstart-ui/package.json",
            "main": "libs/create-kikstart-ui/src/index.ts",
            "assets": ["libs/create-kikstart-ui/*.md"]
          }
        }
      }
    },
    "layout-console": {
      "projectType": "library",
      "root": "libs/layout/console",
      "sourceRoot": "libs/layout/console/src",
      "prefix": "console",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/layout/console/tsconfig.lib.json",
            "project": "libs/layout/console/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/layout/console/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/layout/console/tsconfig.lib.json",
              "libs/layout/console/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/layout/console/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/layout/console/jest.config.js",
            "tsConfig": "libs/layout/console/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/layout/console/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "layout-console": {
      "projectType": "library",
      "root": "libs/layout/console",
      "sourceRoot": "libs/layout/console/src",
      "prefix": "console",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/layout/console/tsconfig.lib.json",
            "project": "libs/layout/console/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/layout/console/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/layout/console/tsconfig.lib.json",
              "libs/layout/console/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/layout/console/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/layout/console/jest.config.js",
            "tsConfig": "libs/layout/console/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/layout/console/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    },
    "themes": {
      "projectType": "library",
      "root": "libs/themes",
      "sourceRoot": "libs/themes/src",
      "prefix": "theme",
      "architect": {
        "build": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx nx run themes:build-package"
              },
              {
                "command": "npx nx run themes:build-css"
              }
            ],
            "parallel": false
          }
        },
        "build-package": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/themes",
            "tsConfig": "libs/themes/tsconfig.lib.json",
            "packageJson": "libs/themes/package.json",
            "main": "libs/themes/src/index.ts",
            "assets": [
              "libs/themes/*.md",
              {
                "glob": "**/*",
                "input": "libs/themes/src/scss",
                "output": "scss"
              },
              {
                "glob": "**/*",
                "input": "libs/themes/src/vendor",
                "output": "vendor"
              }
            ]
          }
        },
        "build-css": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx sass ./dist/libs/themes/scss/dark/index.scss ./dist/libs/themes/css/dark.css"
              },
              {
                "command": "npx sass -s compressed ./dist/libs/themes/scss/dark/index.scss ./dist/libs/themes/css/dark.min.css"
              },
              {
                "command": "npx sass ./dist/libs/themes/scss/pirate/index.scss ./dist/libs/themes/css/pirate.css"
              },
              {
                "command": "npx sass -s compressed ./dist/libs/themes/scss/pirate/index.scss ./dist/libs/themes/css/pirate.min.css"
              },
              {
                "command": "npx sass ./dist/libs/themes/scss/light/index.scss ./dist/libs/themes/css/light.css"
              },
              {
                "command": "npx sass -s compressed ./dist/libs/themes/scss/light/index.scss ./dist/libs/themes/css/light.min.css"
              }
            ],
            "parallel": true
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "libs/themes/tsconfig.lib.json",
              "libs/themes/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/themes/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/themes/jest.config.js",
            "tsConfig": "libs/themes/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "libs/themes/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  },
  "defaultProject": "demo"
}
